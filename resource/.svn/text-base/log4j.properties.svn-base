log4j.rootLogger=INFO,stdout,info

log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d %p [%c] - %m%n

#---------------------------------- MapServer Mapbar Log
log4j.appender.debug=org.apache.log4j.RollingFileAppender
log4j.appender.debug.layout=org.apache.log4j.HTMLLayout 
log4j.appender.debug.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n
log4j.appender.debug.File=./log/debug/debug.html
log4j.appender.debug.MaxFileSize=1024KB
log4j.appender.debug.MaxBackupIndex=10

#---------------------------------- MapServer Mapbar Log
log4j.appender.info=org.apache.log4j.RollingFileAppender
log4j.appender.info.layout=org.apache.log4j.HTMLLayout 
log4j.appender.info.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n
log4j.appender.info.File=./log/info/info.html
log4j.appender.info.MaxFileSize=1024KB
log4j.appender.info.MaxBackupIndex=10



#---------------------------------- error Log
log4j.appender.error=org.apache.log4j.RollingFileAppender
log4j.appender.error.layout=org.apache.log4j.HTMLLayout 
log4j.appender.error.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss,SSS} [%t] [%c] [%p] - %m%n
log4j.appender.error.File=./log/error/error.html
log4j.appender.error.MaxFileSize=1024KB
log4j.appender.error.MaxBackupIndex=10

